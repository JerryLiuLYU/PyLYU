<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="60dp">

    <TextView
        android:id="@+id/textViewholdermaillist"
        android:layout_width="@dimen/cent_80dp"
        android:layout_height="@dimen/cent_40dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="@string/none"
        android:textSize="@dimen/text_30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewholdermaillist2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/none"
        app:layout_constraintBottom_toBottomOf="@+id/textViewholdermaillist"
        app:layout_constraintEnd_toStartOf="@+id/textViewholdermaillist3"
        app:layout_constraintHorizontal_bias="0.586"
        app:layout_constraintStart_toEndOf="@+id/textViewholdermaillist"
        app:layout_constraintTop_toTopOf="@+id/textViewholdermaillist"
        app:layout_constraintVertical_bias="0.476" />

    <TextView
        android:id="@+id/textViewholdermaillist3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="@string/none"
        app:layout_constraintBottom_toBottomOf="@+id/textViewholdermaillist2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textViewholdermaillist2"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>